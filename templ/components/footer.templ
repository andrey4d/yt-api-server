package components


css foooterStyle(){
    color: #ffd700; 
    text-align: center; 
    padding: 3px; 
    background-color: #0047ab; 
    color: white;

}

templ Footer(email string){
    <footer class={ foooterStyle() } >
        <a style="color: #ffd700;" href={ templ.URL( "mailto: " + email)  } >{email}</a>
    </footer>
}